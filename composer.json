{
  "name": "tiny-pixel/acorn-media-tools",
  "type": "package",
  "license": "MIT",
  "description": "Import remote images using Acorn",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "kelly@tinypixel.dev"
    }
  ],
  "keywords": [
    "wordpress",
    "acorn",
    "sage",
    "facebook",
    "instagram",
    "twitter"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/acorn-media-tools/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\AcornMedia\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "tiny-pixel/support": "dev-master"
  },
  "extra": {
    "acorn": {
      "providers": [
        "TinyPixel\\Acorn\\Media\\Providers\\MediaToolsServiceProvider"
      ]
    }
  }
}
